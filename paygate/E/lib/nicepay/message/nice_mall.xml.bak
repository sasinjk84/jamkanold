			encrypt="N" description="승인번호" />
		<column name="ResultCode" mode="AN" size="4" required="Y"
			encrypt="N" description="결과코드" />
		<column name="ResultMsg" mode="AH" size="100" required="Y"
			encrypt="N" description="결과메시지" />
